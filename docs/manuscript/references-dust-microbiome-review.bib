@article{Corona2024,
abstract = {We examined the associations between social support and postpartum mental health in 137 U.S. and foreign-born Latinas in the MADRES pregnancy cohort. We also examined whether language, years in the U.S., and country of birth moderates these relationships. Participants were administered PROMIS support measures 1 month postpartum; the Perceived Stress and Postpartum Distress Measure 3, 6, and 12 months postpartum; and the CESD scale 12 months postpartum. Perceived stress was lower at 6 months postpartum for women reporting higher emotional ( p = 0.01), informational ( p = 0.03), and instrumental support ( p {\textless} 0.001); and lower at 12 months postpartum for women reporting higher emotional support ( p = 0.01). Distress at 6 months was lower in women reporting higher emotional support ( p = 0.03). Interactions suggest that associations were stronger for mothers that speak Spanish, spent fewer years in the U.S., and were born in Central America.},
author = {Corona, Karina and Yang, Tingyu and Dunton, Genevieve and Toledo-Corral, Claudia and Grubbs, Brendan and Eckel, Sandrah P. and Johnston, Jill and Chavez, Thomas and Lerner, Deborah and Lurvey, Nathana and Al-Marayati, Laila and Habre, Rima and Farzan, Shohreh F. and Breton, Carrie V. and Bastain, Theresa M.},
