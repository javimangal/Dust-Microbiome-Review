issn = {24685844},
journal = {Current Opinion in Environmental Science {\&} Health},
keywords = {Built environment,Chemical exposures,Dust exposure,Indoor dust,Semivolatile organic compounds},
mendeley-groups = {Dust Microbiome Papers/Other},
month = {jun},
pages = {1--6},
